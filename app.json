{
  "expo": {
    "name": "MHike",
    "slug": "mhike",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.may.mhike"
    },
    "android": {
    "adaptiveIcon": {
    "foregroundImage": "./assets/img.png",
    "backgroundColor": "#ffffff"
    }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": []
  }
}
